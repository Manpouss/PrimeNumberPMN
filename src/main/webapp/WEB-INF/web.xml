<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/jee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<servlet>
		<servlet-name>PrimeNum</servlet-name>
		<servlet-class>com.formation.servlet.PrimeNum</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PrimeNum</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>
	<jsp-config>
		<taglib>
			<taglib-uri>/WEB-INF/primeNumber2.tld</taglib-uri>
			<taglib-location>/WEB-INF/primeNumber2.tld</taglib-location>
		</taglib>
	</jsp-config>
</web-app>